body{
    background-color: #E3E1E1;
}

.headerelements{
    display: inline-block;
    vertical-align: middle;
}

#menuicon{
    margin-left: 2%;
    width: 2.6%;
    height: auto;
}

#officiallogo{
    margin-left: 2.5%;
    width: 15%;
    height: auto;
}

#searchpart {
    margin-left: 10%;
    display: inline-block;
}

#searchbar {
    height: 35px;
    border: 2px solid black;
    border-radius: 12px;
    background-color: #C9C7C7;
    margin-top: 1.5%;
    width: 600px;
    font-family: "Inder";
    text-align: center;
}

#search {
    height: 42px;
    border-radius: 12px;
    border: 2px solid black;
    background-color: #C9C7C7;
    margin-left: 10px;
    font-family: "Inder";

}

#userprofilepic{
    width: 45px;
    height: auto;
    border-radius: 10px;
    margin-right: 5px;
}

#profileusername{
    margin-right: 15px;
    font-family: "Inder";
}

#logout{
    width: 30px;
    height: auto;
    border: none;
    margin: 0;
    padding: 0;
    background: none;
    cursor: pointer;
    float: right;
}

#logouticon{
    width: 30px;
}

#usercontrol{
    float: right;
    margin-right: 20px;
}

@media only screen and (max-width: 1000px){
    #searchbar{
        width: 540px;
    }
}